<div class="container">
  <div class="row">
    <div class="col-6 mx-auto">
      <%= simple_form_for(pet) do |f| %>
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :species %>
        <%= f.input :found_date, html5: true %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
